<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="roundRadius" format="dimension"/>

    <attr name="strokeColor" format="color"/>

    <attr name="solidColor" format="color"/>

    <attr name="strokeWidth" format="dimension"/>

    <attr name="topleftRadius" format="dimension"/>

    <attr name="toprightRadius" format="dimension"/>

    <attr name="buttomleftRadius" format="dimension"/>

    <attr name="buttomrightRadius" format="dimension"/>

    <attr name="startColor" format="color"/>

    <attr name="centerColor" format="color"/>

    <attr name="endColor" format="color"/>

    <attr name="shape">

        <enum name="rectangle" value="0"></enum>

        <enum name="oval" value="1"></enum>

    </attr>

    <attr name="angle">

        <enum name="LEFT_RIGHT" value="0"></enum>

        <enum name="TOP_BOTTOM" value="1"></enum>

        <enum name="BOTTOM_TOP" value="2"></enum>

        <enum name="RIGHT_LEFT" value="3"></enum>

        <enum name="LT_RB" value="4"></enum>

        <enum name="RT_LB" value="5"></enum>

        <enum name="LB_RT" value="6"></enum>

        <enum name="RB_LT" value="7"></enum>

    </attr>

    <attr name="gradientType">

        <enum name="linear" value="0"></enum>

        <enum name="radial" value="1"></enum>

        <enum name="sweep" value="2"></enum>

    </attr>


    <declare-styleable name="TextShape">

        <attr name="roundRadius"/>
        <attr name="strokeColor"/>
        <attr name="solidColor"/>
        <attr name="strokeWidth"/>
        <attr name="shape"/>
        <attr name="topleftRadius"/>
        <attr name="toprightRadius"/>
        <attr name="buttomleftRadius"/>
        <attr name="buttomrightRadius"/>
        <attr name="startColor"/>
        <attr name="endColor"/>
        <attr name="centerColor"/>
        <attr name="angle"/>
        <attr name="gradientType"/>

    </declare-styleable>


    <declare-styleable name="LeanTextView">
        <attr name="degree" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LinerLayoutShape">

        <attr name="roundRadius"/>
        <attr name="strokeColor"/>
        <attr name="solidColor"/>
        <attr name="strokeWidth"/>
        <attr name="shape"/>
        <attr name="topleftRadius"/>
        <attr name="toprightRadius"/>
        <attr name="buttomleftRadius"/>
        <attr name="buttomrightRadius"/>
        <attr name="startColor"/>
        <attr name="endColor"/>
        <attr name="centerColor"/>
        <attr name="angle"/>
        <attr name="gradientType"/>

    </declare-styleable>

    <declare-styleable name="EdiTextShape">

        <attr name="roundRadius"/>
        <attr name="strokeColor"/>
        <attr name="solidColor"/>
        <attr name="strokeWidth"/>
        <attr name="shape"/>
        <attr name="topleftRadius"/>
        <attr name="toprightRadius"/>
        <attr name="buttomleftRadius"/>
        <attr name="buttomrightRadius"/>
        <attr name="startColor"/>
        <attr name="endColor"/>
        <attr name="centerColor"/>
        <attr name="angle"/>
        <attr name="gradientType"/>

    </declare-styleable>

    <declare-styleable name="RelativeLayoutShape">

        <attr name="roundRadius"/>
        <attr name="strokeColor"/>
        <attr name="solidColor"/>
        <attr name="strokeWidth"/>
        <attr name="shape"/>
        <attr name="topleftRadius"/>
        <attr name="toprightRadius"/>
        <attr name="buttomleftRadius"/>
        <attr name="buttomrightRadius"/>
        <attr name="startColor"/>
        <attr name="endColor"/>
        <attr name="centerColor"/>
        <attr name="angle"/>
        <attr name="gradientType"/>

    </declare-styleable>
</resources>